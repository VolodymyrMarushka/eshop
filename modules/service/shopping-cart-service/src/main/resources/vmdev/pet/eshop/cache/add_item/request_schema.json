{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "cart",
    "item"
  ],
  "properties": {
    "cart": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string"
        }
      }
    },
    "item": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "id",
        "quantity"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "quantity": {
          "type": "string"
        }
      }
    }
  }
}
